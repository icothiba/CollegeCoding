f the input is:

5 2 4 6 8 10

the output is:

10,8,6,4,2,


#include <iostream>
#include <vector>   // Must include vector library to use vectors
using namespace std;

int main() {
   vector<int> userInts; // A vector to hold the user's input integers
   int count;
   
   cin >> count;

   for (int i = 0; i < count; ++i) {
       int num;
       cin >> num;
       userInts.push_back(num);
   }

   for (int i = userInts.size() - 1; i >= 0; --i) {
       cout << userInts[i];
       if (i != 0) {
           cout << ",";
       }
   }

   cout << "," << endl;

   
   return 0;
}
