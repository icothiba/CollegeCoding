If the input is:

5 20 40 60 80 100

the output is:

all multiples of 10

If the input is:

5 10 25 30 40 55

the output is:

mixed values


#include <iostream>
#include <vector>
using namespace std;

bool IsVectorMult10(const vector<int>& myVec) {
    for (int num : myVec) {
        if (num % 10 != 0) {
            return false;
        }
    }
    return true;
}

bool IsVectorNoMult10(const vector<int>& myVec) {
    for (int num : myVec) {
        if (num % 10 == 0) {
            return false;
        }
    }
    return true;
}

int main() {
    int size;
    cin >> size;
    
    vector<int> myList(size);
    for (int i = 0; i < size; ++i) {
        cin >> myList[i];
    }
    
    bool allMult10 = IsVectorMult10(myList);
    bool noMult10 = IsVectorNoMult10(myList);
    
    if (allMult10) {
        cout << "all multiples of 10" << endl;
    } else if (noMult10) {
        cout << "no multiples of 10" << endl;
    } else {
        cout << "mixed values" << endl;
    }
    
    return 0;
}
