#include <iostream>
#include <iomanip>
using namespace std;

int main() {
int temp, sum = 0, count = 0;
   cin >> temp;
   int max = temp;
   sum = temp;
   count++;
   
   while(temp >= 0){
      cin >> temp;
      
      if (temp > max){
         max = temp;
      }
      if (temp >= 0){
         sum += temp;
         count++;
      }
   }
   cout << fixed << setprecision(2);
   cout << max << " " << (double)sum / count << endl;
   return 0;
  

   return 0;
}
