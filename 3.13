#include <iostream>
using namespace std;

int main() {
   int highwayNumber;
   int primaryHighway;
   
   cin >> highwayNumber;

   if (highwayNumber < 1 || highwayNumber > 999) {
      cout << highwayNumber << " is not a valid interstate highway number." << endl;
   }
   else {
      if (highwayNumber >= 100) {
         cout << "I-" << highwayNumber << " is auxiliary, serving I-";
         primaryHighway = highwayNumber % 100;
         cout << primaryHighway;
      }
      else {
         cout << "I-" << highwayNumber << " is primary";
         primaryHighway = highwayNumber;
      }
   
      
      if (primaryHighway % 2 == 0)
         cout << ", going east/west." << endl;
      else
         cout << ", going north/south." << endl;
      
   }
      
      

   return 0;
}
