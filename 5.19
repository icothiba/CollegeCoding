If the input is:

5 hey hi Mark hi mark

the output is:

hey - 1
hi - 2
Mark - 1
hi - 2
mark - 1


#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {

   vector<string> userWords;
   vector<int> wordFreq;
   unsigned int numWords;
   unsigned int i;
   unsigned int j; 
   string word;
   
   cin >> numWords;
   for (i = 0; i < numWords; ++i) {
      cin >> word;
      userWords.push_back(word);
      wordFreq.push_back(0);
   }
   
   for (i = 0; i < numWords; ++i) {
      for (j = 0; j < numWords; ++j) {
         if (userWords.at(i) == userWords.at(j)) {
            wordFreq.at(i) = wordFreq.at(i) + 1;
         }
      }
   }

   for (i = 0; i < numWords; ++i) {
      cout << userWords.at(i) << " - " << wordFreq.at(i) << endl;
   }

   return 0;
}
