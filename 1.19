Right-facing arrow

#include <iostream>
using namespace std;

int main() {
   int baseDigit; 0
   int headDigit; 1

   cout << "      " << headDigit << endl;
   cout << "       " << headDigit<<headDigit << endl;
   cout << "        " << baseDigit<<baseDigit<<baseDigit<<baseDigit<<baseDigit << endl;
   cout << "         " << endl;
   cout << "        " << endl;
   cout << "       " << endl;
   cout << "      " << endl;

   return 0;
}
